{
    "name": "Smile Sketch",
    "version": "1.1.0",
    "description": "Sketch资源导出、图层排序、图层校准插件",
    "homepage": "http://github.com/1217950746/Smile-Sketch",
    "author": "ONEO",
    "authorEmail": "1217950746@qq.com",
    "identifier": "oneo.smile-sketch",
    "commands": [
        {
            "script": "script.js",
            "handlers": {
                "actions": {
                    "DocumentSaved": "DocumentSaved"
                }
            }
        },
        {
            "name": "导出",
            "shortcut": "ctrl shift e",
            "identifier": "Export",
            "handler": "Export",
            "script": "script.js"
        },
        {
            "name": "导入",
            "shortcut": "ctrl shift d",
            "identifier": "Import",
            "handler": "Import",
            "script": "script.js"
        },
        {
            "name": "画板排序",
            "shortcut": "ctrl shift a",
            "identifier": "Sort",
            "handler": "Sort",
            "script": "script.js"
        },
        {
            "name": "页面排序",
            "shortcut": "ctrl shift p",
            "identifier": "PageSort",
            "handler": "PageSort",
            "script": "script.js"
        },
        {
            "name": "Github",
            "identifier": "LinkGithub",
            "handler": "LinkGithub",
            "script": "script.js"
        },
        {
            "name": "反馈",
            "identifier": "LinkFeedback",
            "handler": "LinkFeedback",
            "script": "script.js"
        }
    ],
    "menu": {
        "title": "Smile Sketch",
        "items": [
            "Export",
            "Import",
            "Sort",
            "PageSort",
            "-",
            "LinkGithub",
            "LinkFeedback"
        ]
    }
}