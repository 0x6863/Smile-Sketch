{
    "name": "smile-sketch",
    "description": "简单的使用 Sketch 的一些功能",
    "version": "2.0.0",
    "engines": {
        "sketch": ">=49.0"
    },
    "skpm": {
        "name": "Smile Sketch",
        "manifest": "src/manifest.json",
        "main": "smile-sketch.sketchplugin",
        "assets": [
            "assets/**/*"
        ],
        "sketch-assets-file": "sketch-assets/icons.sketch"
    },
    "scripts": {
        "build": "skpm-build && skpm-link",
        "start": "skpm-build --watch"
    },
    "devDependencies": {
        "@skpm/builder": "^0.8.0"
    },
    "author": "ONEO <oneo.me@outlook.com>"
}
