{
    "name": "smile-sketch",
    "description": "更加快捷的使用 Sketch 的一些功能",
    "version": "2.0.0",
    "engines": {
        "sketch": ">=80.0"
    },
    "skpm": {
        "name": "Smile Sketch",
        "manifest": "src/manifest.json",
        "main": "smile-sketch.sketchplugin",
        "assets": [
            "assets/**/*"
        ],
        "sketch-assets-file": "sketch-assets/icons.sketch"
    },
    "scripts": {
        "dev": "skpm-build --watch",
        "build": "skpm-build && skpm-link"
    },
    "devDependencies": {
        "@skpm/builder": "^0.8.0",
        "glob-parent": ">=6.0.2"
    },
    "author": "ONEO <oneo.me@outlook.com>"
}
