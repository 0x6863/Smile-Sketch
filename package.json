{
    "name": "smile-sketch",
    "displayName": "Smile Sketch",
    "version": "1.0.0",
    "description": "Sketch资源导出、图层排序、图层校准插件",
    "homepage": "http://github.com/1217950746/Smile-Sketch",
    "author": "ONEO",
    "authorEmail": "1217950746@qq.com",
    "license": "MIT",
    "actions": {
        "DocumentSaved": "onDocumentSaved"
    },
    "menus": [
        {
            "name": "导出全部",
            "handler": "exportAll",
            "shortcut": "shift cmd d"
        },
        {
            "name": "导入全部",
            "handler": "importAll",
            "shortcut": "shift cmd c"
        },
        {
            "name": "文档排序",
            "handler": "sorting",
            "shortcut": "shift cmd f"
        },
        {
            "name": "-"
        },
        {
            "name": "微博",
            "handler": "linkWeibo"
        },
        {
            "name": "Github",
            "handler": "linkGithub"
        }
    ],
    "devDependencies": {
        "fs-extra": "^2.1.2"
    }
}